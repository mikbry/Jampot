{
  "extends": [
      "airbnb-base",
      "plugin:import/errors",
      "prettier"
  ],
  "env": {
      "browser": false,
      "es6": true,
      "jquery": false,
      "node": true
  },
  "globals": {
      "logger": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "prettier"
  ],
  "rules": {
      "quotes": [
          "error",
          "double"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": 1,
      "prefer-const": "error",
      "no-var": "error",
      "prettier/prettier": ["error", {
          "arrowParens": "always",
          "trailingComma": "all"
      }]
  }
}
